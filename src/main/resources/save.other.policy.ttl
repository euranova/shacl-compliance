save-ex:WebPolicyPermission  a  orcp:Permission , owl:NamedIndividual ;
                          save:action          save:Use , save:Transform ;
                          save:controller      save-ex:SomeController ;
                          save:data            save:BrowsingBehavior , save:LinkClicked ;
                          save:hasDataSubject  save-ex:SomeUser ;
                          save:purpose         save:OptimisationForConsumer , save:PersonalisedBenefits , save:OptimisationForController .

save-ex:CreditCardPermission
    a                orcp:Permission , owl:NamedIndividual ;
    save:action          save:Transform , save:Use ;
    save:controller      save-ex:SomeController ;
    save:data            save:PhysicalAddress , save:Age , save:Name ;
    save:hasDataSubject  save-ex:SomeUser ;
    save:purpose         save:OptimisationForController , save:OptimisationForConsumer , save:PersonalisedBenefits .


save-ex:SomeUser  a            dpv:DataSubject ;
               rdfs:label      "Some user" ;
               skos:prefLabel  "Some user" .

save-ex:SomeController  a      dpv:DataController ;
                     rdfs:label      "Some controller" ;
                     skos:prefLabel  "Some controller" .

