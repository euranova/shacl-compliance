                                                        ]
                                         ] ;
                           sh:construct  "PREFIX :<http://example.com/privacy-policy-model/>\nPREFIX ex:<http://example.org/shacl_examples#>\nCONSTRUCT {\n\t$this :nChildren ?total .\n\t$this :nPermitted ?granted .\n\t$this :nProhibited ?prohibited .\n             $this :answer ?perm .\n}\nWHERE {\n\t{SELECT $this (COUNT(*) AS ?total)\n\tWHERE {\n\t\t?s :parent $this .\n\t}\n\tgroup by $this\n\t}\n\t{SELECT $this (SUM(IF(?p,1,0)) AS ?granted)\n\tWHERE {\n\t\t?s :parent $this .\n\t\tbind(exists { ?s :answer \"permitted\" } as ?p)\n\t}\n\tgroup by $this\n\t}\n\t\t\t{\n\tSELECT $this (SUM(IF(?proh,1,0)) AS ?prohibited)\n\tWHERE {\n\t\t?s :parent $this .\n\t\tbind(exists { ?s :answer \"prohibited\" } as ?proh)\n\t}\n\tgroup by $this\n\t}\n\tBIND ( IF ( ?total = ?granted, \"granted\", IF( ?granted = 0 && ?prohibited > 0, \"prohibited\" , IF ( ?total > ?granted && ?granted > 0, \"part-granted\", 'not granted' ) )) AS ?perm )\n}\n" ;
